# Required: provide your license key before running cobalt-docker.sh
COBALTSTRIKE_LICENSE="REPLACE_WITH_YOUR_COBALTSTRIKE_LICENSE"

# Required: shared teamserver password
TEAMSERVER_PASSWORD="REPLACE_WITH_YOUR_TEAMSERVER_PASSWORD"

# Optional: REST API username
REST_API_USER="csrestapi"

# Optional: host port that maps to SERVICE_PORT in the container
REST_API_PUBLISH_PORT="50443"

# Optional startup hardening controls
SERVICE_BIND_HOST="0.0.0.0"
SERVICE_PORT="50443"
UPSTREAM_HOST="127.0.0.1"
UPSTREAM_PORT="50050"
HEALTHCHECK_URL="https://127.0.0.1:50443/health"
HEALTHCHECK_INSECURE="true"
